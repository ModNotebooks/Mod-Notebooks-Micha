<div class="notebook__inner">
  <div class="notebook__sleevewrap">
    <div class="notebook__sleeve">
      <h2 class="notebook__title">
        {{#if name}}
          {{name}}
        {{else}}
          Untitled
        {{/if}}
      </h2>
      {{#if processed}}
        <span class="notebook__date">{{format-date processedOn 'MMM. D, YYYY'}}</span>
      {{else}}
        <span class="notebook__status">{{humanize state}}</span>
      {{/if}}
    </div>
  </div>
</div>
