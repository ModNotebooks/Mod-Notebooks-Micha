{{#each model}}
  {{#link-to 'notebook' this class="notebook"}}
    <div class="notebook__sleevewrap">
      <div class="notebook__sleeve">
        <h2 class="notebook__title">
          {{#if name}}
            {{name}}
          {{else}}
            Untitled
          {{/if}}
        </h2>
        <span class="notebook__status">{{currState}}</span>
      </div>
    </div>
  {{/link-to}}
{{/each}}
