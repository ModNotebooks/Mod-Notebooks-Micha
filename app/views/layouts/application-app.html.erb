<!DOCTYPE html>
<html>
<head>

  <!--
  Designed/Developed by 3DROPS
  w: http://3dro.ps
  e: hello@3dro.ps
  t: @3drops
  -->

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <title>Mod Notebooks</title>

  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no, minimal-ui">
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag "http://fast.fonts.net/cssapi/7650c855-f883-471e-97df-c7366fa30f5d.css" %>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= javascript_include_tag "head" %>
</head>
<body>
  <div id="main" class="l-app-main">
    <header class="l-app-header">
      <div class="l-app-logo">
        <a href="/" class="logo--app"></a>
      </div>
      <aside class="l-app-side-nav">
        <div class="l-app-side-nav-item">
          Item
        </div>
      </aside>
      <div class="l-app-navtoggle">
        <a href="" class="settingstoggle">
          <span class="settingstoggle__inner"></span>
        </a>
      </div>
    </header>
    <section class="l-app-content">

      <div class="l-notebooks">
        <div class="l-notebooks-wrap">

          <div class="l-grid-wrap">
            <div class="l-grid">

              <div class="l-grid-item--notebook js-grid-item">
                <a href="javascript:void(0);" data-ember-action="12" class="notebook--standin">
                  <span class="notebook--standin__plus"></span>
                </a>
              </div>

              <% 2.times do %>
              <div class="l-grid-item--notebook js-grid-item">
                <a id="ember816" class="ember-view notebook gray" href="/notebooks/1">
                  <div class="notebook__inner">
                    <div class="notebook__sleevewrap">
                      <div class="notebook__sleeve">
                        <h2 class="notebook__title">Video Sketches</h2>
                          <span class="notebook__date">Mar. 18, 2014</span>
                      </div>
                    </div>
                  </div>
                </a>
              </div>
              <% end %>

            </div>
          </div>

        </div>
      </div>

    </section>
  </div>
<!--   <div id="settings"></div> -->

  <%= cdn_jquery %>
  <%#= javascript_include_tag "application" %>
</body>
</html>
