    <form  class="form" style="max-width: 500px;">
      <div class="form__control">
        <input type="text" class="form__control__input">
        <label for="field" class="form__control__label">Full Name</label>
        <!-- <span class="form__error">I am an error</span> -->
      </div>
      <div class="form__control">
        <input type="text" class="form__control__input">
        <label for="field" class="form__control__label">Address</label>
      </div>
      <div class="form__controlgroup">
        <div class="form__control--onethird">
          <input type="text" class="form__control__input">
          <label for="field" class="form__control__label">City</label>
          <!-- <span class="form__error">I am an error</span> -->
        </div>
        <div class="form__control--onethird">
          <input type="text" class="form__control__input">
          <label for="field" class="form__control__label">State</label>
          <!-- <span class="form__error">I am an error</span> -->
        </div>
        <div class="form__control--onethird">
          <input type="text" class="form__control__input">
          <label for="field" class="form__control__label">Postal Code</label>
        </div>
      </div>
      <div class="form__control">
        <input type="text" class="form__control__input">
        <label for="field" class="form__control__label">Country</label>
      </div>
      <div class="form__actions">
        <button type="submit" class="button--form">Save</button>
      </div>
    </form>

    <hr>

    <form  class="form" style="max-width: 500px;">
      <div class="form__radiogroup">
        <div class="form__radio">
          <input type="radio" name="handleMethod" id="radio1" class="form__radio__input">
          <label for="radio1" class="form__radio__labelwrap">
            <div class="form__radio__label">Send it back to me (+$10)</div>
            <div class="form__radio__custominput">
              <span></span>
            </div>
          </label>
        </div>
        <div class="form__radio">
          <input type="radio" name="handleMethod" id="radio2" class="form__radio__input">
          <label for="radio2" class="form__radio__labelwrap">
            <div class="form__radio__label">Send it back to me (+$10)</div>
            <div class="form__radio__custominput">
              <span></span>
            </div>
          </label>
        </div>
        <div class="form__radio">
