<section class="l-notebooks">
  <div class="l-notebooks-list">
    {{#each model}}
      <div class="l-notebooks-listitem">
        {{#link-to 'notebook' this class="notebook"}}
          {{partial 'partials/notebook'}}
        {{/link-to}}
      </div>
    {{/each}}
    <div class="l-notebooks-listitem">
      <div class="notebook--standin">
        <span class="notebook--standin__plus"></span>
      </div>
    </div>
  </div>
</section>
